div>\r\n        <div class=\"px-2 py-2 bg-gradient-to-r from-blue-500 to-teal-400\">\r\n          Цена\r\n          <br>\r\n          <span class=\"price\">{{ product.price }}\r\n              </span>\r\n        </div>\r\n        <div class=\"flex justify-content-between\">\r\n          <RouterLink to='/' class=\"custom-link\">\r\n            Посмотреть\r\n          </RouterLink>\r\n          <RouterLink to='/' class=\"custom-link\">\r\n            Посмотреть\r\n          </RouterLink>\r\n          <RouterLink to='/' class=\"custom-link\">\r\n            Посмотреть\r\n          </RouterLink>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [],\r\n      loadedImages: {}, // Массив для хранения загруженных изображений\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      const token = localStorage.getItem('token');\r\n      const headers = {\r\n        'Authorization': `Bearer ${token}`\r\n      };\r\n      const response = await axios.get('/products/my', { headers });\r\n      this.products = response.data;\r\n      console.log(response.data);\r\n\r\n    } catch (error) {\r\n      console.error('Error fetching products:', error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.price {\r\n  color: green;\r\n}\r\n\r\n.product-image {\r\n  width: 300px;\r\n  height: 300px;\r\n  object-fit: cover;\r\n  object-position: center;\r\n}\r\n\r\n.custom-link {\r\n  font-size: 14px;\r\n  background-color: #4299e1; /* Цвет фона */\r\n  color: #ffffff; /* Цвет текста */\r\n  font-weight: bold; /* Жирный текст */\r\n  padding: 8px 16px; /* Отступы внутри ссылки */\r\n  border-radius: 4px; /* Закругление углов */\r\n  text-decoration: none; /* Убрать подчеркивание под ссылкой */\r\n}\r\n\r\n.custom-link:hover {\r\n  background-color: #3182ce; /* Изменение цвета фона при наведении */\r\n}\r\n\r\n.cards > div {\r\n  b