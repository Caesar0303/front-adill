{"ast":null,"code":"import axios from \"axios\";\nimport PostData from \"@/components/posts/PostData.vue\";\nexport default {\n  components: {\n    PostData\n  },\n  data() {\n    return {\n      products: []\n    };\n  },\n  async created() {\n    try {\n      const token = localStorage.getItem('token');\n      const headers = {\n        'Authorization': `Bearer ${token}`\n      };\n      const response = await axios.get('/products', {\n        headers\n      });\n      this.products = response.data;\n      console.log(response.data);\n    } catch (error) {\n      console.error('Error fetching products:', error);\n    }\n  }\n};","map":{"version":3,"names":["axios","PostData","components","data","products","created","token","localStorage","getItem","headers","response","get","console","log","error"],"sources":["d:\\Programs\\OSPanel\\domains\\back-adil\\src\\views\\ProductList.vue"],"sourcesContent":["<script>\r\nimport axios from \"axios\";\r\nimport PostData from \"@/components/posts/PostData.vue\";\r\n\r\nexport default {\r\n  components: {PostData},\r\n  data() {\r\n    return {\r\n      products: [],\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      const token = localStorage.getItem('token');\r\n      const headers = {\r\n        'Authorization': `Bearer ${token}`\r\n      }\r\n      const response = await axios.get('/products',{ headers });\r\n      this.products = response.data;\r\n      console.log(response.data);\r\n    } catch (error) {\r\n      console.error('Error fetching products:', error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <h2>Все объявления</h2>\r\n  <div class=\"container mx-auto mt-0\">\r\n    <div class=\"cards flex flex-wrap justify-center\">\r\n      <div v-for=\"product in products\" :key=\"product.id\" class=\"w-30 overflow-hidden shadow-lg m-4 transition transform hover:-translate-y-1 hover:shadow-2xl\">\r\n        <img :src=\"'http://127.0.0.1:8000/' + product.image\" alt=\"Product Image\" class=\"product-image\">\r\n        <div class=\"cards px-6 py-2 bg-white\">\r\n            <br>\r\n            <span class=\"font-bold text-xl mb-5 text-gray-800\">{{ product.name }}</span>\r\n            <br>\r\n            <br>\r\n            <p class=\"text-gray-700 text-base mb-4\">\r\n              Описание\r\n              <br>\r\n              {{ product.description }}\r\n            </p>\r\n            <p class=\"text-gray-700 text-base mb-4\">\r\n              Количество\r\n              <br>\r\n              {{ product.quantity }}\r\n            </p>\r\n            <p class=\"text-gray-700 text-base mb-4\">\r\n              Категория\r\n              <br>\r\n              {{ product.category.name }}\r\n            </p>\r\n          </div>\r\n          <div class=\"px-2 py-2 bg-gradient-to-r from-blue-500 to-teal-400\">\r\n            Цена\r\n            <br>\r\n            <span class=\"price\">{{ product.price }}\r\n              </span>\r\n          </div>\r\n        <RouterLink to='/' class=\"custom-link\">\r\n          Посмотреть\r\n        </RouterLink>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n//\r\n\r\n.product-image {\r\n  width: 300px;\r\n  height: 300px;\r\n  object-fit: cover;\r\n  object-position: center;\r\n}\r\n\r\n.custom-link {\r\n  background-color: #4299e1; /* Цвет фона */\r\n  color: #ffffff; /* Цвет текста */\r\n  font-weight: bold; /* Жирный текст */\r\n  padding: 8px 16px; /* Отступы внутри ссылки */\r\n  border-radius: 4px; /* Закругление углов */\r\n  text-decoration: none; /* Убрать подчеркивание под ссылкой */\r\n}\r\n\r\n.custom-link:hover {\r\n  background-color: #3182ce; /* Изменение цвета фона при наведении */\r\n}\r\n\r\n.cards > div {\r\n  border: 2px solid #94c3f1; /* добавляем границу */\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease, border-color 0.3s ease; /* плавный переход для трансформации, тени и цвета границы */\r\n}\r\n\r\n.cards > div:hover {\r\n  transform: scale(1.05); /* немного увеличиваем размер */\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2); /* увеличиваем тень */\r\n  border-color: #007bff; /* изменяем цвет границы при наведении */\r\n}\r\n</style>\r\n"],"mappings":"AACA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,iCAAiC;AAEtD,eAAe;EACbC,UAAU,EAAE;IAACD;EAAQ,CAAC;EACtBE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,MAAMC,OAAM,GAAI;QACd,eAAe,EAAG,UAASH,KAAM;MACnC;MACA,MAAMI,QAAO,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,WAAW,EAAC;QAAEF;MAAQ,CAAC,CAAC;MACzD,IAAI,CAACL,QAAO,GAAIM,QAAQ,CAACP,IAAI;MAC7BS,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACP,IAAI,CAAC;IAC5B,EAAE,OAAOW,KAAK,EAAE;MACdF,OAAO,CAACE,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;IAClD;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}