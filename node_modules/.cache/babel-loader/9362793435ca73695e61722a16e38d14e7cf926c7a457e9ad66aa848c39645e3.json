{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  components: {},\n  data() {\n    return {\n      name: '',\n      phone: '',\n      description: '',\n      quantity: '',\n      price: '',\n      category: '',\n      categories: []\n    };\n  },\n  async created() {\n    try {\n      const response = await axios.get('/product/create');\n      this.categories = response.data;\n    } catch (error) {\n      console.error('Ошибка при загрузке категорий:', error);\n    }\n  },\n  methods: {\n    async submit() {\n      try {\n        const token = localStorage.getItem('token');\n        console.log(token);\n        const productData = {\n          name: this.name,\n          phone: this.phone,\n          description: this.description,\n          quantity: this.quantity,\n          price: this.price,\n          category: this.category\n        };\n        const headers = {\n          'Authorization': `Bearer ${token}`\n        };\n        const response = await axios.post('/product/store', productData, {\n          headers\n        });\n        if (response.status === 200) {\n          this.name = '';\n          this.phone = '';\n          this.description = '';\n          this.quantity = '';\n          this.price = '';\n        }\n        console.log('Product created:', response.data);\n      } catch (error) {\n        console.error(\"Ошибка при создании продукта:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","components","data","name","phone","description","quantity","price","category","categories","created","response","get","error","console","methods","submit","token","localStorage","getItem","log","productData","headers","post","status"],"sources":["d:\\Programs\\OSPanel\\domains\\back-adil\\src\\views\\ProductCreate.vue"],"sourcesContent":["<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  components: {\r\n  },\r\n  data() {\r\n    return {\r\n      name: '',\r\n      phone: '',\r\n      description: '',\r\n      quantity: '',\r\n      price: '',\r\n      category: '',\r\n      categories: [],\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      const response = await axios.get('/product/create');\r\n      this.categories = response.data;\r\n    } catch (error) {\r\n      console.error('Ошибка при загрузке категорий:', error);\r\n    }\r\n  },\r\n  methods: {\r\n    async submit() {\r\n      try {\r\n        const token = localStorage.getItem('token');\r\n        console.log(token);\r\n        const productData = {\r\n          name: this.name,\r\n          phone: this.phone,\r\n          description: this.description,\r\n          quantity: this.quantity,\r\n          price: this.price,\r\n          category: this.category,\r\n        };\r\n\r\n        const headers = {\r\n          'Authorization': `Bearer ${token}`\r\n        };\r\n\r\n        const response = await axios.post('/product/store', productData, { headers });\r\n        if (response.status === 200) {\r\n          this.name = '';\r\n          this.phone = '';\r\n          this.description = '';\r\n          this.quantity = '';\r\n          this.price = '';\r\n        }\r\n        console.log('Product created:', response.data);\r\n      } catch (error) {\r\n        console.error(\"Ошибка при создании продукта:\", error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div class=\"flex items-center h-screen bg-gray-100\">\r\n    <div class=\"w-full max-w-lg p-5 mx-auto bg-white rounded-lg shadow\">\r\n      <form @submit.prevent=\"submit\" class=\"mb-4\">\r\n        <!-- Выпадающий список для выбора категории -->\r\n        <div class=\"mb-4\">\r\n          <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"category\">\r\n            Категория\r\n          </label>\r\n          <br>\r\n          <select v-model=\"category\" class=\"block w-full bg-gray-200 border border-gray-200 rounded py-2 px-4 leading-tight focus:outline-none focus:bg-white focus:bor